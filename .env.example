# AKBID Lab System Environment Variables

# Application
VITE_APP_NAME=AKBID Lab System
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Sistem Manajemen Laboratorium AKBID

# Environment
NODE_ENV=development
VITE_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Database
VITE_DATABASE_SCHEMA=public

# Security
VITE_JWT_SECRET=your_jwt_secret_here
VITE_ENCRYPTION_KEY=your_encryption_key_here

# File Storage
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,ppt,pptx,jpg,jpeg,png,gif

# Development Features
VITE_DEV_MODE=true
VITE_DEV_TOOLBAR=true
VITE_DEV_ROLE_SWITCH=true
VITE_DEV_QUICK_LOGIN=true

# PWA
VITE_PWA_ENABLED=true
VITE_PWA_CACHE_NAME=akbid-lab-cache-v1

# API
VITE_API_TIMEOUT=10000
VITE_API_RETRY_COUNT=3

# Logging
VITE_LOG_LEVEL=debug
VITE_LOG_CONSOLE=true

# Performance
VITE_PAGINATION_SIZE=10
VITE_SEARCH_DEBOUNCE=300

# Lab Configuration
VITE_LAB_COUNT=9
VITE_DEPO_COUNT=1
VITE_MAX_CONCURRENT_BOOKINGS=3

# Notification
VITE_NOTIFICATION_TIMEOUT=5000
VITE_NOTIFICATION_POSITION=top-right